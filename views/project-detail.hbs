                <div class="right-side">
                    <h3>Duration</h3>

                    <!-- Duration -->
                    <div class="duration">
                        <div class="calender">
                            <img src="/public/assets/calender.png" alt="calender">
                            <p style="font-size: 15pt; margin-top: -30px; color: black;">{{project.startdate}} s/d
                                {{project.enddate}}</p>
                        </div>

                        <div class="clock">
                            <img src="/public/assets/clock-regular.svg" alt="clock">
                            <p style="font-size: 15pt;">
                                {{project.duration}}
                                |
                                {{#if project.author.name}}
                                {{project.author.name}}
